    <div class="hero" id="hero">
    <div class="hero-content">
        <h1 id="hero-title">Building Tomorrow's Banjul Together</h1>
        <p class="lead" id="hero-description">Empowering Communities, Transforming Lives</p>
    </div>
</div>

<script>
    document.addEventListener("DOMContentLoaded", function() {
        const heroData = [
            {
                title: "Building Tomorrow's Banjul Together",
                description: "Join us in creating impactful change.",
                background: "url('background1.jpg')"
            },
            {
                title: "Empowering Communities, Transforming Lives",
                description: "Together, we can achieve more.",
                background: "url('background2.jpg')"
            },
            {
                title: "Sustainable Development for All",
                description: "Investing in our community's future.",
                background: "url('background3.jpg')"
            },
            {
                title: "Innovating for a Brighter Future",
                description: "Innovation starts with collaboration.",
                background: "url('background4.jpg')"
            },
            {
                title: "Your Voice Matters: Get Involved!",
                description: "Your participation is key to progress.",
                background: "url('background5.jpg')"
            },
            {
                title: "Together We Rise: Unity in Action",
                description: "Let's work together for a better tomorrow.",
                background: "url('background6.jpg')"
            }
        ];

        let index = 0;

        function updateHeroContent() {
            const currentData = heroData[index];
            document.getElementById("hero-title").textContent = currentData.title;
            document.getElementById("hero-description").textContent = currentData.description;
            document.getElementById("hero").style.backgroundImage = currentData.background;
            index = (index + 1) % heroData.length; // Loop back to the start
        }

        // Update every 5 seconds
        setInterval(updateHeroContent, 5000);
        updateHeroContent(); // Initial display
    });
</script>

